  "scripts": {
    "editorconfig": "editorconfig-cli",
    "prod": "gulp build --production",
    "dev": "gulp build",
    "eslint": "eslint source/js/**/*.js",
    "stylelint": "stylelint \"source/styles/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "start": "gulp start",
    "images": "gulp images",
    "prettier": "prettier --write \"source/(scss|js)/**/*.(scss|css|js|json)\""
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/scss/**/*.scss"
